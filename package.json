{
  "name": "i-css",
  "version": "0.3.1",
  "main": "dist/index.js",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "react-scripts": "1.0.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "dependencies": {
    "free-style": "^2.3.1"
  },
  "scripts": {
    "start": "NODE_PATH=src PORT=8080 react-scripts start",
    "build": "NODE_PATH=src react-scripts build",
